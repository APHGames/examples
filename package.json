{
  "name": "ni-aph",
  "author": "Adam Veseck√Ω",
  "version": "4.3.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm run prebuild && npm run serve",
    "serve": "parcel examples/view/*.html examples/view/*.pug --out-dir build",
    "prebuild": "node scripts/prebuild.js",
    "deploy": "npm run prebuild && parcel build examples/view/*.html examples/view/*.pug --out-dir build --public-url PUBLICURL --no-cache --no-source-maps && node scripts/fix-links.js",
    "lint": "eslint ./src ./libs --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "pixi-sound": "^3.0.5",
    "pixi.js": "^5.3.3",
    "three": "^0.120.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-standard": "4.0.1",
    "parcel": "1.12.4",
    "typescript": "^4.0.3"
  }
}
